package serveur;

import GarageCorba.IGarageRemotePOA;
import GarageCorba.Voiture;

public class ServiceImp extends IGarageRemotePOA{

	public Voiture[] voitures=new Voiture[3];
	

	@Override
	public void Retour(int matricule) 
	{
		// TODO Auto-generated method stub
		
	}

	@Override
	public void Location(int matricule) 
	{
		// TODO Auto-generated method stub
		
	}

	

	@Override
	public Voiture[] afficheVoitures() 
	{
		// TODO Auto-generated method stub
		return voitures;
	}

	@Override
	public void deplacerVoiture(Voiture v) 
	{
		// TODO Auto-generated method stub
		
	}

	@Override
	public void nouvelleVoiture(int matricule, String etat, String src_garage,
			String name) {
		voitures[voitures.length]=new Voiture(matricule,etat, src_garage,name);
		
	}

}
